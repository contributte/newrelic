{
  "name": "contributte/newrelic",
  "description": "NewRelic PHP agent integration for Nette Framework",
  "keywords": [
    "nette",
    "newrelic",
    "monitoring"
  ],
  "homepage": "https://github.com/contributte/newrelic",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrik Votoček",
      "homepage": "http://patrik.votocek.cz"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Tomáš Jacík",
      "homepage": "https://tomasjacik.cz"
    }
  ],
  "support": {
    "issues": "https://github.com/contributte/newrelic/issues"
  },
  "require": {
    "php": ">=8.2",
    "ext-newrelic": "*",
    "nette/di": "^3.2.0",
    "nette/application": "^3.2.0",
    "tracy/tracy": "^2.10.0"
  },
  "require-dev": {
    "contributte/console": "^0.10.0",
    "contributte/event-dispatcher": "^0.10.0",
    "contributte/phpstan": "^0.1.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.4.0",
    "mockery/mockery": "^1.6.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\NewRelic\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "suggest": {
    "contributte/console": "For a nice formated newrelic logs from console commands",
    "contributte/event-dispatcher": "For a nice formated newrelic logs from console commands"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.0-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  }
}
